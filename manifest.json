{
  "name": "TuEnvioExtension",
  "version": "1.0",
  "description": "Para Gestionar Notificaciones",
  "browser_action": {
    "default_title": "Activar Notificacion de tuEnvio"       
  },   
  "permissions": [
    "*://*.tuenvio.cu/*",
    "notifications",
    "storage"
],
"background": {
  "scripts": ["bundle.js"]
},
  
  "manifest_version": 2
}